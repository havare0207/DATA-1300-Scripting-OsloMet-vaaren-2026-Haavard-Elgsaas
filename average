def calculate_averager(a, b, c):
    total = a + b + c
    average = total / 3
    return average

a = float(input("Skriv inn fÃ¸rste tall: "))
b = float(input("Skriv inn andre tall: "))
c = float(input("Skriv inn tredje tall: "))

result = calculate_averager(a, b, c)
print("Gjennomsnittet er:", result)

